{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}"
        },
        {
            "name": "help",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "--help"
            ]
        },
        {
            "name": "invalid cli args",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "--ddd222"
            ]
        },
        {
            "name": "ping",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "ping"
            ]
        },
        {
            "name": "create config",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "--create-config",
                "--config",
                "./.local/default_config.json",
            ]
        },
        {
            "name": "ollama profile override",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "--config",
                "./.local/profile-override.json",
            ]
        },
        {
            "name": "nullest.com ping",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "--config",
                "./.local/nullest-ping.json",
                "--debug",
                "ping"
            ]
        },
        {
            "name": "nullest.com deepseekcoder",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "--debug",
                "--config",
                "./.local/nullest-deepseek-coder.json",
                "ping"
            ]
        },
        {
            "name": "profile",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "--debug",
                "please create an example compose.yml to run super docker servers"
            ]
        },
        {
            "name": "nil",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "--config",
                "./.local/github-token.json",
                "ping pong"
            ]
        }
    ]
}